<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.7" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JSPX" value="true" />
      </map>
    </option>
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="750" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo\b\s+@jp\s+.*" />
    <pattern pattern="\bfixme\b \b@jp\b .*" />
    <filter name="@JP">
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
</application>